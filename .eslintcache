lue={item.representative_signature} name=\"representative_signature\" index={i} setter={handleInput} classAppend=\"tw-pl-6 pw-pr-2 tw-pb-3 tw-pt-1 tw-justify-center\" canvasProps={{ width: 360, height: 100 }} />\r\n                              </div>\r\n\r\n                              <FormInline label=\"Огноо\" type=\"date\" value={item.submitDate} name=\"submitDate\" index={i} setter={handleInput} classAppend=\"tw-w-full tw-max-w-sm\" classInput=\"tw-w-40\" invalid={validate && checkInvalid(item.submitDate)} />\r\n                           </div>\r\n                        </ExpandableContainer>\r\n\r\n                        <div className=\"tw-flex tw-items-center\">\r\n                           <ButtonTooltip tooltip=\"Хасах\" beforeSVG={<MinusCircleSVG className=\"tw-w-8 tw-h-8 tw-transition-colors tw-duration-300\" />} onClick={() => handleRemove(i)} classButton=\"tw-text-red-500 active:tw-text-red-600\" />\r\n                        </div>\r\n                     </div>\r\n                  )}\r\n\r\n                  <div className=\"tw-flex tw-justify-end tw-items-center tw-py-1 tw-px-2\">\r\n                     <div className=\"tw-text-xs tw-italic tw-text-gray-600 tw-mr-2\">\r\n                        ААН-ийн төлөөлөгчид\r\n                     </div>\r\n                     <ButtonTooltip tooltip=\"Шинээр нэмэх\" beforeSVG={<PlusCircleSVG className=\"tw-w-8 tw-h-8 tw-transition-colors tw-duration-300\" />} onClick={handleAdd} classAppend=\"tw-mr-2 tw-my-0.5\" classButton=\"tw-text-green-500 active:tw-text-green-600\" />\r\n                  </div>\r\n\r\n                  <div className=\"tw-flex tw-justify-end\">\r\n                     <SaveButton onClick={handleSubmit} buttonAppend=\"tw-m-6\" />\r\n                  </div>\r\n               </animated.div>\r\n            )}\r\n         </Transition>\r\n      </div>\r\n   )\r\n}\r\n\r\nexport const Notice = ({ checkList, handleCheckList, order, orderDisplay, sub, notice }) => (\r\n   <div classN