%;\r\n      }\r\n    }\r\n    .nameTitle{\r\n      // font-weight:500;\r\n      span{\r\n        margin-left:15px;\r\n        font-weight:400;\r\n      }\r\n    }\r\n    .contentPar{\r\n      margin-top:15px;\r\n      .items{\r\n        margin:10px 0px; \r\n      }\r\n      .Content{\r\n        padding:0px 50px;\r\n        .Titles{\r\n          margin:25px 0px;\r\n          text-align:center;\r\n        }\r\n        ul{\r\n          list-style-type:decimal;\r\n          .items{\r\n            .ZorchilPar{\r\n              margin:10px 0px;\r\n              .titleBig{\r\n                font-weight:500;\r\n              }\r\n              .childPar{\r\n                display:flex;\r\n                align-items:center;\r\n                margin-top:10px;\r\n                .child{\r\n                  display:flex;\r\n                  align-items:center;\r\n                  .smTitle{\r\n                    margin-left:5px;\r\n                  }\r\n                  .radio{\r\n                    cursor:pointer;\r\n                    width:16px;\r\n                    height:16px;\r\n                  }\r\n                }\r\n                .childA{\r\n                  margin-left:30px;\r\n                }\r\n              }\r\n            }\r\n          }\r\n          .form-control{\r\n            font-size:0.9em;\r\n          }\r\n          .smUl{\r\n            padding-left:50px;\r\n            list-style-type:upper-roman;\r\n          }\r\n        }\r\n        \r\n\r\n      }\r\n      \r\n    }\r\n    @media (max-width:768px){\r\n      padding: 15px 10px;\r\n      .contentPar{\r\n        .Content{\r\n          padding: 0px 25px;\r\n          ul{\r\n            .smUl{\r\n              padding-left: 35px;\r\n            }\r\n          }\r\n        }\r\n      }\r\n    }\r\n\r\n    @media print{\r\n      margin-bottom:30px;\r\n      background-color:white;\r\n      max-width:1200px;\r\n      margin-top:20px;\r\n      font-size:15px;\r\n      padding:15px 80px;